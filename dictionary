a = {"Taro":1985, "Hanako":1986}
print a

b = a["Taro"]
print b

a["Jiro"] = 1988
print a

a["Hanako"] = 1990
print a

c = {1:1991, 2:1992}
print c

d = c[2]
print d

f = {1:1991, 2:1992, 3:1993 ,4:1994}
print f

f[5] = 1995
print f

x = 1

if x > 0 and x < 3:
    f[6] = 1996
    print f
elif x >= 3 and x <= 9:
    f[7] = 1997
    print f
else:
    f[8] = 1998
    print f


